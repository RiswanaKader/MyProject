package com.javaconcepts;

public class LoopingStatement {
	int f=5;
 public void sub() {
	 for(int g=100;g<=105;g++)
	 {
		 System.out.println(g);
	 } 
 }
 public void sub1()
 {
	 while(f<=10) {
		 System.out.println("Java");
		 f++;jh
	 }
	 System.out.println();
 }
public void sub2(){
	do {
		System.out.print("Hello ");
		f--;
		}
	while(f<5);
}

 public static void main(String[] args)
 {
	LoopingStatement loop=new LoopingStatement();
	loop.sub2();
	loop.sub();
	loop.sub1();
	
 }
}
